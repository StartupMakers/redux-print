{
  "name": "redux-print",
  "version": "1.0.0",
  "description": "Simple Redux solution to prevent a CSS transition issues during a page printing",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build:lib": "cross-env NODE_ENV=production babel src --out-dir lib",
    "build": "npm run clean && npm run build:lib",
    "prepublish": "npm run build"
  },
  "author": "Startup Makers Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/StartupMakers/redux-print.git"
  },
  "bugs": "https://github.com/StartupMakers/redux-print/issues",
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^1.0.7",
    "rimraf": "^2.3.4"
  }
}
